{
  "expo": {
    "name": "AIndriver",
    "slug": "AIndriver",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/root/icon.png",
    "scheme": "aindriver",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/root/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/root/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.qb1ty_as.AIndriver"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/root/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Разрешите AIndriver доступ к вашей камере"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "bbf694c0-3f99-447c-9615-ee7ec5fbdcf6"
      }
    },
    "owner": "qb1ty_as"
  }
}
